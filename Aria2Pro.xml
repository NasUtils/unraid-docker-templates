<?xml version="1.0"?>
<Container>
    <Name>aria2pro</Name>
    <Repository>docker.mirrors.ustc.edu.cn/p3terx/aria2-pro</Repository>
    <Registry>https://hub.docker.com/r/p3terx/aria2-pro</Registry>
    <Network>host</Network>
    <Privileged>false</Privileged>
    <Support>https://p3terx.com/archives/docker-aria2-pro.html</Support>
    <Project>https://github.com/P3TERX/docker-aria2-pro</Project>
    <Overview>aria2pro</Overview>
    <Description>
        更好的Aria2容器镜像
    </Description>
    <Category>download:</Category>
    <Icon>https://imgcdn.p3terx.com/avatar/c42fdfa8aca47ec27a7c35dfa7d1dc51</Icon>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/aria2pro</HostDir>
            <ContainerDir>/config</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
        <Volume>
            <HostDir>/mnt/user/download</HostDir>
            <ContainerDir>/downloads</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Environment>
        <Variable>
            <Value>0</Value>
            <Name>PUID</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>0</Value>
            <Name>PGID</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>P3TERX</Value>
            <Name>RPC_SECRET</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>6800</Value>
            <Name>RPC_PORT</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>6888</Value>
            <Name>LISTEN_PORT</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>64M</Value>
            <Name>DISK_CACHE</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>move</Value>
            <Name>SPECIAL_MODE</Name>
            <Mode/>
        </Variable>
    </Environment>
</Container>
